snippet imava "import ava" b
import test from 'ava'
endsnippet

snippet test "test" b
test('$1', t => {
	$2
})
endsnippet

snippet test-async "test async" b
test('$1', async t => {
	$2
})
endsnippet

snippet testbe "test before each" b
test.beforeEach(t => {
	$2
})
endsnippet

snippet testae "test after each" b
test.afterEach(t => {
	$2
})
endsnippet


